{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "Esto es para un curso  del manejo de Webpak",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "manual-build": "webpack --entry ./index.js --output ./bundle --mode development",
    "build-production": "webpack --mode production",
    "build-external": "webpack --config ./external/webpack.config.js",
    "build-multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build-css": "webpack --config ./css-style-loader/webpack.config.js",
    "build-plugins": "webpack --config ./plugins/webpack.config.js",
    "build-dev": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "build-dev-auto": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build-babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build-react": "webpack-dev-server --config ./react/webpack.config.js",
    "build-url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js",
    "build-prepo": "webpack-dev-server --config ./preprocesadores/webpack.config.js",
    "build-prevent-dupli": "webpack --config ./prevent-duplication/webpack.config.js",
    "build-dll": "webpack --config ./dll/webpack.dll.config.js",
    "build-dll-pro": "webpack --config ./dll/webpack.config.js",
    "build-dynamic-dll": "webpack --config ./dynamic-imports/webpack.dll.config.js",
    "build-dynamic": "webpack --config ./dynamic-imports/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Brayan Bernal",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^3.10.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.1",
    "postcss-loader": "^7.0.2",
    "postcss-nested": "^4.2.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^7.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.1"
  }
}
